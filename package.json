{
	"name": "lowcode",
	"displayName": "lowcode",
	"description": "lowcode tool, support ChatGPT",
	"author": "wjkang <ruoxieme@gmail.com>",
	"version": "1.7.6",
	"icon": "asset/icon.png",
	"publisher": "wjkang",
	"repository": "https://github.com/lowcoding/lowcode-vscode",
	"main": "./build/extension.js",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "webpack --mode production",
		"compile:tsc": "tsc -p ./",
		"dev": "yarn --cwd \"webview-react\" dev",
		"build": "yarn --cwd \"webview-react\" build",
		"lint": "eslint src --ext ts",
		"watch": "webpack --mode development",
		"pretest": "yarn run compile:tsc",
		"test:js": "node ./build/test/runTest.js",
		"test": "node ./build/test/runTest.js"
	},
	"engines": {
		"vscode": "^1.72.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"ChatGPT",
		"openai",
		"GPT",
		"yapi",
		"yapi to ts",
		"typescript to json",
		"ts to json",
		"json to ts",
		"json to typescript",
		"json mock",
		"typescript mock",
		"ts mock",
		"mock",
		"generate code",
		"json",
		"lowcode",
		"low-code",
		"form-render",
		"amis",
		"formily"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"commands": [
			{
				"command": "lowcode.generateCodeByWebview",
				"title": "可视化生成代码"
			},
			{
				"command": "lowcode.generateCode",
				"title": "生成代码"
			},
			{
				"command": "lowcode.openDownloadMaterials",
				"title": "下载物料"
			},
			{
				"command": "lowcode.addSnippet",
				"title": "添加代码片段"
			},
			{
				"command": "lowcode.quickGenerateBlock",
				"title": "快速创建区块"
			},
			{
				"command": "lowcode.showChatGPTView",
				"title": "ChatGPT"
			},
			{
				"command": "lowcode.hideChatGPTView",
				"title": "关闭",
				"icon": "$(close)"
			},
			{
				"command": "lowcode.openSettingsChatGPT",
				"title": "Open Setting",
				"icon": "$(settings-gear)"
			},
			{
				"command": "lowcode.askChatGPT",
				"title": "Ask ChatGPT"
			},
			{
				"command": "lowcode.askChatGPTWithTemplate",
				"title": "Ask ChatGPT With Template"
			},
			{
				"command": "lowcode.runSnippetScript",
				"title": "Run Snippet Script"
			},
			{
				"command": "lowcode.runSnippetScriptOnExplorer",
				"title": "执行脚本"
			}
		],
		"menus": {
			"editor/context": [
				{
					"submenu": "lowcode/editor/context",
					"group": "1_lowcode@1"
				}
			],
			"explorer/context": [
				{
					"submenu": "lowcode/explorer/context",
					"group": "2_lowcode@1",
					"when": "explorerResourceIsFolder"
				}
			],
			"lowcode/editor/context": [
				{
					"command": "lowcode.generateCodeByWebview",
					"group": "lowcode@1"
				},
				{
					"command": "lowcode.generateCode",
					"group": "lowcode@2"
				},
				{
					"command": "lowcode.openDownloadMaterials",
					"group": "lowcode@3"
				},
				{
					"command": "lowcode.addSnippet",
					"group": "lowcode@4"
				},
				{
					"command": "lowcode.askChatGPT",
					"group": "lowcode@5"
				},
				{
					"command": "lowcode.askChatGPTWithTemplate",
					"group": "lowcode@5"
				},
				{
					"command": "lowcode.runSnippetScript",
					"group": "lowcode@6"
				}
			],
			"lowcode/explorer/context": [
				{
					"command": "lowcode.quickGenerateBlock",
					"group": "lowcode@1"
				},
				{
					"command": "lowcode.generateCodeByWebview",
					"group": "lowcode@3"
				},
				{
					"command": "lowcode.runSnippetScriptOnExplorer",
					"group": "lowcode@2"
				},
				{
					"command": "lowcode.openDownloadMaterials",
					"group": "lowcode@4"
				}
			],
			"view/title": [
				{
					"command": "lowcode.hideChatGPTView",
					"when": "view == lowcode.chatGPTView",
					"group": "navigation@4"
				},
				{
					"command": "lowcode.openSettingsChatGPT",
					"when": "view == lowcode.chatGPTView",
					"group": "navigation@3"
				}
			]
		},
		"submenus": [
			{
				"id": "lowcode/editor/context",
				"label": "Low Code",
				"icon": "$(octoface)"
			},
			{
				"id": "lowcode/explorer/context",
				"label": "Low Code",
				"icon": "$(octoface)"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "lowcodeApp",
					"title": "lowcode",
					"icon": "asset/activitybar.svg"
				}
			]
		},
		"views": {
			"lowcodeApp": [
				{
					"id": "lowcode.welcome",
					"name": "home",
					"icon": "$(octoface)",
					"when": "!lowcode.showChatGPTView"
				},
				{
					"type": "webview",
					"id": "lowcode.chatGPTView",
					"name": "chatGPT",
					"when": "lowcode.showChatGPTView"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "lowcode.welcome",
				"contents": "[打开文件夹](command:lowcode.openFolderForceNewWindow)\n[创建应用](command:lowcode.openScaffold)\n[ChatGPT](command:lowcode.showChatGPTView)\n[设置](command:lowcode.openConfig)\n查看[文档](https://lowcoding.gitee.io)以学习如何在 VS Code 中如何使用 lowcode。"
			}
		],
		"configuration": {
			"type": "object",
			"title": "lowcode",
			"properties": {
				"lowcode.hostname": {
					"type": "string",
					"default": "api.openai.com",
					"description": "chatGPT hostname"
				},
				"lowcode.apiPath": {
					"type": "string",
					"default": "/v1/chat/completions",
					"description": "chatGPT api path"
				},
				"lowcode.apiKey": {
					"type": "string",
					"default": "",
					"description": "chatGPT api key"
				},
				"lowcode.model": {
					"type": "string",
					"default": "gpt-3.5-turbo",
					"description": "chatGPT model"
				},
				"lowcode.maxTokens": {
					"type": "number",
					"default": 2000,
					"minimum": 100,
					"markdownDescription": "chatGPT max tokens"
				},
				"lowcode.temperature": {
					"type": "number",
					"default": 0.3,
					"minimum": 0,
					"maximum": 1,
					"description": "chatGPT temperature"
				},
				"lowcode.syncFolder": {
					"type": "string",
					"default": "",
					"description": "sync folder"
				}
			}
		}
	},
	"devDependencies": {
		"@types/copy-paste": "^1.1.30",
		"@types/debug": "^4.1.5",
		"@types/ejs": "^3.0.4",
		"@types/fs-extra": "^9.0.1",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/prettier": "^2.0.2",
		"@types/readable-stream": "^2.3.9",
		"@types/urijs": "^1.19.9",
		"@types/vscode": "1.72.0",
		"@typescript-eslint/eslint-plugin": "^4.18.0",
		"@typescript-eslint/parser": "^4.18.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.24.0",
		"eslint-plugin-prettier": "^3.3.1",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"prettier": "^2.2.1",
		"ts-loader": "^8.0.3",
		"typescript": "^4.3.5",
		"vscode-test": "^1.3.0",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"copy-paste": "^1.3.0",
		"directory-tree": "^2.2.4",
		"ejs": "^3.1.3",
		"execa": "^4.0.3",
		"fs-extra": "^9.0.1",
		"generate-schema": "^2.6.0",
		"globby": "^11.0.1",
		"is-url": "^1.2.4",
		"json-schema-to-typescript": "^9.1.1",
		"strip-comments": "^2.0.1",
		"strip-json-comments": "^3.1.1",
		"tar": "^6.0.5",
		"typescript-json-schema": "^0.50.1"
	}
}