{
  "name": "lowcode",
  "displayName": "lowcode",
  "description": "低代码工具",
  "author": "wjkang <ruoxieme@gmail.com>",
  "version": "1.2.7",
  "icon": "asset/icon.png",
  "publisher": "wjkang",
  "repository": "https://github.com/lowcoding/lowcode-vscode",
  "main": "./build/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "webpack --mode production",
    "compile:tsc": "tsc -p ./",
    "webview:dev-vue": "yarn --cwd \"webview-vue\" dev",
    "webview:build-vue": "yarn --cwd \"webview-vue\" build",
    "webview:dev-react": "yarn --cwd \"webview-react\" dev",
    "webview:build-react": "yarn --cwd \"webview-react\" build",
    "lint": "eslint src --ext ts",
    "watch": "webpack --mode development",
    "pretest": "yarn run compile:tsc",
    "test:js": "node ./build/test/runTest.js",
    "test": "node ./build/test/runTest.js"
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "yapi",
    "typescript",
    "generate code",
    "json",
    "lowcode"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "yapi-code.generateCodeByWebview",
        "title": "可视化生成代码"
      },
      {
        "command": "yapi-code.generateCode",
        "title": "生成代码"
      },
      {
        "command": "lowcode.openDownloadMaterials",
        "title": "下载物料"
      },
      {
        "command": "yapi-code.addSnippet",
        "title": "添加代码片段"
      },
      {
        "command": "lowcode.quickGenerateBlock",
        "title": "快速创建区块"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "lowcode/editor/context",
          "group": "1_lowcode@1"
        }
      ],
      "explorer/context": [
        {
          "submenu": "lowcode/explorer/context",
          "group": "2_lowcode@1",
          "when": "explorerResourceIsFolder"
        }
      ],
      "lowcode/editor/context": [
        {
          "command": "yapi-code.generateCodeByWebview",
          "group": "lowcode@1"
        },
        {
          "command": "yapi-code.generateCode",
          "group": "lowcode@2"
        },
        {
          "command": "lowcode.openDownloadMaterials",
          "group": "lowcode@3"
        },
        {
          "command": "yapi-code.addSnippet",
          "group": "lowcode@4"
        }
      ],
      "lowcode/explorer/context": [
        {
          "command": "lowcode.quickGenerateBlock",
          "group": "lowcode@1"
        },
        {
          "command": "yapi-code.generateCodeByWebview",
          "group": "lowcode@2"
        },
        {
          "command": "lowcode.openDownloadMaterials",
          "group": "lowcode@3"
        }
      ]
    },
    "submenus": [
      {
        "id": "lowcode/editor/context",
        "label": "Low Code"
      },
      {
        "id": "lowcode/explorer/context",
        "label": "Low Code"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lowcodeApp",
          "title": "lowcode",
          "icon": "asset/activitybar.svg"
        }
      ]
    },
    "views": {
      "lowcodeApp": [
        {
          "id": "lowcode.welcome",
          "name": "Welcome"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "lowcode.welcome",
        "contents": "[打开文件夹](command:lowcode.openFolderForceNewWindow)\n[创建应用](command:lowcode.openScaffold)\n[设置](command:lowcode.openConfig)\n查看[文档](https://lowcoding.gitee.io)以学习如何在 VS Code 中如何使用 lowcode。"
      }
    ]
  },
  "devDependencies": {
    "@types/copy-paste": "^1.1.30",
    "@types/debug": "^4.1.5",
    "@types/ejs": "^3.0.4",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/prettier": "^2.0.2",
    "@types/readable-stream": "^2.3.9",
    "@types/urijs": "^1.19.9",
    "@types/vscode": "1.42.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.3.1",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.3.5",
    "vscode-test": "^1.3.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "copy-paste": "^1.3.0",
    "directory-tree": "^2.2.4",
    "ejs": "^3.1.3",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "generate-schema": "^2.6.0",
    "globby": "^11.0.1",
    "is-url": "^1.2.4",
    "json-schema-to-typescript": "^9.1.1",
    "strip-comments": "^2.0.1",
    "strip-json-comments": "^3.1.1",
    "tar": "^6.0.5",
    "typescript-json-schema": "^0.50.1"
  }
}